//* https://www.staging-typescript.org/tsconfig

{
  "compilerOptions": {
    "lib": [ "es2015" ],
    "target": "es5",
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
    ,"emitDecoratorMetadata": true
    ,"experimentalDecorators": true,
  }
}
