{
  "name": "typescript-node-boilerplate",
  "version": "1.0.0",
  "description": "⚡️ minimal boilerplate based on typescript with node ⚡️",
  "main": "dist/app.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/app.js\"",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production pm2 start dist/app.js --name 'test1' -e err.log -o out.log --time",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "jong",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "child_process": "^1.0.2",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "fs": "^0.0.1-security",
    "got": "^13.0.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.1",
    "method-override": "^3.0.0",
    "multer": "^1.4.4",
    "mysql2": "^3.6.0",
    "node-schedule": "^2.1.1",
    "npm": "^9.8.1",
    "readline": "^1.3.0",
    "rimraf": "^5.0.1",
    "socket.io": "^4.7.2",
    "typeorm": "^0.3.17",
    "validatorjs": "^3.22.1"
  }
}
