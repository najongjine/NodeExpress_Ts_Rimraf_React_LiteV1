{
  "name": "typescript-node-boilerplate",
  "version": "1.0.0",
  "description": "⚡️ minimal boilerplate based on typescript with node ⚡️",
  "main": "dist/app.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/app.js\"",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "jong",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "tsc-watch": "^5.0.3"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.23",
    "bcrypt": "^5.0.1",
    "child_process": "^1.0.2",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "fs": "^0.0.1-security",
    "got": "^12.0.3",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "multer": "^1.4.4",
    "mysql2": "^2.3.3",
    "node-schedule": "^2.1.0",
    "readline": "^1.3.0",
    "rimraf": "^3.0.2",
    "socket.io": "^4.4.1",
    "typeorm": "^0.3.5",
    "validatorjs": "^3.22.1"
  }
}
